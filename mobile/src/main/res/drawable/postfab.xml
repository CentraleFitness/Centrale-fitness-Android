<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient
                android:type="linear"
                android:angle="0"
                android:startColor="@color/ourOrange"
                android:endColor="@color/ourRed" />
        </shape>

    </item>
    <item android:gravity="center"
        android:bottom="20dp"
        android:top="20dp"
        android:left="20dp"
        android:right="20dp"

        >
        <bitmap android:src="@drawable/fabicon"
            android:gravity="fill_horizontal"
            />

    </item>

</layer-list>